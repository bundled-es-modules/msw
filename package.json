{
  "name": "@bundled-es-modules/msw",
  "version": "2.0.0",
  "description": "mirror of msw, bundled and exposed as ES module",
  "author": "Pascal Schilp <pascalschilp@gmail.com>",
  "main": "index.js",
  "devDependencies": {
    "@web/dev-server": "^0.1.30",
    "msw": "^0.0.0-fetch.rc-12",
    "esbuild": "^0.17.18"
  },
  "scripts": {
    "start": "wds",
    "prebuild": "msw init ./ --save=false && mv mockServiceWorker.js sw.js",
    "build": "esbuild --bundle node_modules/msw/lib/core/index.mjs --outfile=index.js --format=esm"
  },
  "files": [
    "index.js",
    "sw.js"
  ]
}
